<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="example test suite" verbose="1">

    <parameter name="chrome-browser" value="chrome"></parameter>
    <parameter name="firefox-browser" value="firefox"></parameter>
    <parameter name="url" value="https://www.espnqa.com/?src=com&amp;_adblock=true&amp;espn=cloud"></parameter>


    <test name="espn test login">
        <classes>
           <class name="TestLogin"></class>
        </classes>
    </test>
    <test name="espn test logout">
        <classes>
            <class name="TestLogOut"></class>
        </classes>
    </test>
    <test name="espn test delete Account">
        <classes>
            <class name="TestDeleteProfile"></class>
        </classes>
    </test>
</suite>